{
  "name": "portfolio-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon --typescript public/icons",
    "build:analyze": "ANALYZE=true npm run build",
    "apollo:download-schema": "apollo client:download-schema schema.graphql",
    "apollo:generate-types": "rm -rf __generated__ && apollo client:codegen --target typescript --outputFlat",
    "apollo:sync": "npm run apollo:download-schema && npm run apollo:generate-types",
    "codegen:generate": "graphql-codegen",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint '**/*.{js,jsx,ts,tsx}' --fix",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "jest",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:test": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@material-ui/core": "^4.12.3",
    "@next/bundle-analyzer": "^10.0.5",
    "@reduxjs/toolkit": "^1.6.0",
    "axios": "^0.21.1",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "next": "10.2.0",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^7.0.1",
    "prettier": "^2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.5.16",
    "react-redux": "^7.2.4",
    "react-use": "^15.3.8",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5",
    "swr": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-knobs": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.14",
    "@svgr/cli": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.0",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "apollo": "^2.32.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "cypress": "^6.2.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.7",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nock": "^13.0.6",
    "start-server-and-test": "^1.11.0",
    "storybook-addon-styled-component-theme": "^1.3.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint -- --quiet --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
